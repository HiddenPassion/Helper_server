{
  "extends": "google",
  "rules": {
    "linebreak-style": 0,
    "no-tabs": 0,
    "eol-last": 0,
    "max-len": [2, { "code": 100 }],
    "camelcase": [2, { "properties": "always" }],
    "no-trailing-spaces": 0,
    "require-jsdoc": 0,
    "no-throw-literal": 0,
    "new-cap": 0,
    "object-curly-spacing": 0,
    "comma-dangle": ["error", "always-multiline"]
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "spread": true,
      "experimentalObjectRestSpread": true
    }
  }
}
